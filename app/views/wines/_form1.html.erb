<%= form_with(model: wine, local: true) do |form| %>
  <% if wine.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(wine.errors.count, "error") %> prohibited this wine from being saved:</h2>

      <ul>
        <% wine.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field ">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>


  <div class="field ">
    <%  wine.wines_one.each do |x| %>
      <li><%=  x %></li><br>
    <%end%>
    
    <%= form.fields_for :wine_strains do |wine_strains_form|%>
      <%= wine_strains_form.text_field :percentage %>
    <%end%>
  </div>
 
  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
